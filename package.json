{
  "name": "virtual-tryon-app",
  "version": "1.0.0",
  "description": "Real-time virtual try-on application with camera feed",
  "main": "server/index.js",
  "scripts": {
    "dev": "node server/index.js",
    "server": "node server/index.js",
    "db:migrate": "node server/db/migrate.js",
    "db:seed": "node server/db/seed.js",
    "products:fashion:download:kagglehub": "python3 server/scripts/downloadFashionDatasetFromKaggleHub.py",
    "products:fashion:import": "node server/scripts/importFashionDatasetToSupabase.js",
    "products:fashion:upload:images": "FASHION_IMPORT_METADATA=false FASHION_UPLOAD_IMAGES=true node server/scripts/importFashionDatasetToSupabase.js",
    "cleanup:appledouble": "bash scripts/cleanup-appledouble.sh",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up",
    "docker:down": "docker compose down",
    "docker:prod:pull": "docker compose -f docker-compose.prod.yml --env-file .env.prod pull",
    "docker:prod:up": "docker compose -f docker-compose.prod.yml --env-file .env.prod up -d --remove-orphans",
    "docker:prod:down": "docker compose -f docker-compose.prod.yml --env-file .env.prod down"
  },
  "keywords": [
    "virtual-tryon",
    "camera",
    "real-time",
    "fashion"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.870.0",
    "@aws-sdk/s3-request-presigner": "^3.870.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "socket.io": "^4.6.1",
    "unzipper": "^0.12.3"
  }
}
