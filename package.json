{
  "name": "virtual-tryon-app",
  "version": "1.0.0",
  "description": "Real-time virtual try-on application with camera feed",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": ["virtual-tryon", "camera", "real-time", "fashion"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "socket.io": "^4.6.1",
    "multer": "^1.4.5-lts.1",
    "canvas": "^2.11.2",
    "@tensorflow/tfjs-node": "^4.11.0",
    "@tensorflow-models/body-pix": "^2.2.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1"
  }
}
